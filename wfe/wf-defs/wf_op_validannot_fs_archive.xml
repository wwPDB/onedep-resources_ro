                        <wf:dataObjectsLocation>
                            <wf:location dataID="D2" type="input"/>
                        </wf:dataObjectsLocation>
                    </wf:process>
                </wf:task>

	      <!-- Look at archive directory, update maps on entry before usual processing - write back to archive with map updates -->
